orden()contenido(titulo del top down);
orden(1)contenido(primer nodo);
orden(1.1)contenido(primer hijo del primer nodo);
orden(1.1.1)contenido(primer hijo del primer hijo del primer nodo);
orden(1.2)contenido(segundo hijo del primer nodo);
orden(2)contenido(segundo nodo);
orden(2.1)contenido(primer hijo del segundo nodo);
